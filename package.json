{
  "name": "assert-ts",
  "version": "0.1.0",
  "description": "Invariant and non-null/undefined assertion check with type guards",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fram-x/assert-ts.git"
  },
  "keywords": [
    "assert",
    "invariant",
    "precondition",
    "guard",
    "assume",
    "null-check",
    "type guard",
    "flow typing"
  ],
  "author": "Bj√∏rn Egil Hansen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fram-x/assert-ts/issues"
  },
  "homepage": "https://github.com/fram-x/assert-ts#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
